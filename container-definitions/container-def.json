[
    {
      "name": "spring-boot-docker",
      "image": "docker.io/aamershaikh/docker-spring-boot:latest",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": { 
          "awslogs-group" : "/ecs/frontend-container",
          "awslogs-region": "us-east-1"
        }
      }
    }
]